<h1>All Articles</h1>
<table>
    <%= link_to 'New Article', new_article_path %> </button>
    <tr>
        <th>Title</th>
    </tr>
    <ul>
        <% @articles.each do |article| %>
        <li>
            <%= link_to article.title, article_path(article) %><br/>
        </li>
        <li>
            <%= link_to 'Edit', edit_article_path(article) %></button>
        </li>
        <% end %> 
    </ul>
</table>


 
  
